							   function createSetSelectionModal() {
					  const slot = prompt("–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –∫–æ–º–ø–ª–µ–∫—Ç–∞ (1-4):");
					  if (!slot) return;

					  const sets = JSON.parse(localStorage.getItem("moswar_saved_sets") || "{}");
					  const set = sets[String(slot)];
					  if (!set || !set.length) {
						alert("‚ùå –ö–æ–º–ø–ª–µ–∫—Ç –Ω–µ –Ω–∞–π–¥–µ–Ω –∏–ª–∏ –ø—É—Å—Ç");
						return;
					  }

					  localStorage.setItem("moswar_active_set", String(slot));
					  updateSetIndicator();

					  let refueled = 0;
					  let launched = 0;

					  const delay = (ms) => new Promise(res => setTimeout(res, ms));

					  const sendAll = async () => {
						for (let { carId, rideId } of set) {
						  try {
							const fuel = await fetch(`/automobile/car/${carId}/`, {
							  credentials: "include"
							}).then(r => r.text());

							const fuelMatch = fuel.match(/–¢–æ–ø–ª–∏–≤–æ:[^0-9]*([0-9]+)/i);
							const currentFuel = fuelMatch ? parseInt(fuelMatch[1], 10) : 0;

							if (currentFuel === 0) {
							  await fetch(`/automobile/buypetrol/${carId}/`, {
								method: "POST",
								credentials: "include",
								headers: { "Content-Type": "application/x-www-form-urlencoded" },
								body: `__ajax=1&return_url=%2Fautomobile%2Fcar%2F${carId}`
							  });
							  refueled++;
							  console.log(`‚õΩ –ó–∞–ø—Ä–∞–≤–ª–µ–Ω–∞ –º–∞—à–∏–Ω–∞ ${carId}`);
							  await delay(500);
							}

							await fetch("/automobile/ride/", {
							  method: "POST",
							  credentials: "include",
							  headers: { "Content-Type": "application/x-www-form-urlencoded" },
							  body: `car=${carId}&direction=${rideId}&__ajax=1`
							});

							launched++;
							console.log(`üì§ –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–∞ –º–∞—à–∏–Ω–∞ ${carId} ‚ûú ${rideId}`);
							await delay(500);

						  } catch (err) {
							console.warn(`‚ùå –û—à–∏–±–∫–∞ —Å –º–∞—à–∏–Ω–æ–π ${carId}:`, err);
						  }
						}

						alert(`‚úÖ –ö–æ–º–ø–ª–µ–∫—Ç ${slot} –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω.\n–û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ: ${launched}, –∑–∞–ø—Ä–∞–≤–ª–µ–Ω–æ: ${refueled}`);
					  };

					  sendAll();
					}


    function updateSetIndicator() {
        const slot = localStorage.getItem("moswar_active_set") || "‚Äì";
        const label = document.querySelector(".set-indicator-label");
        if (label) {
            const sets = JSON.parse(localStorage.getItem("moswar_saved_sets") || "{}");
            const count = sets[slot]?.length || 0;
            label.textContent = slot !== "‚Äì" ? `–ö–æ–º–ø–ª–µ–∫—Ç: ${slot} (${count})` : "–ö–æ–º–ø–ª–µ–∫—Ç: ‚Äì";
        }
    }



    function applySavedSet(slot) {
        const sets = JSON.parse(localStorage.getItem("moswar_saved_sets") || "{}");
        const selected = sets[slot];
        if (selected && selected.length) {
            // –û—á–∏—â–∞–µ–º —Ç–µ–∫—É—â–∏–µ –≤—ã–±—Ä–∞–Ω–Ω—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã
            document.querySelectorAll('input.dynamic-checkbox:checked').forEach(cb => cb.checked = false);
            // –ü—Ä–∏–º–µ–Ω—è–µ–º —Å–æ—Ö—Ä–∞–Ω–µ–Ω–Ω—ã–π –∫–æ–º–ø–ª–µ–∫—Ç
            selected.forEach(s => {
                const carInput = document.querySelector(`input[name="car"][value="${s.carId}"]`);
                if (carInput) {
                    const li = carInput.closest("li");
                    const dirInput = li.querySelector(`input[name="direction"][value="${s.rideId}"]`);
                    if (dirInput) {
                        const checkbox = li.querySelector('input.dynamic-checkbox');
                        if (checkbox) checkbox.checked = true;
                    }
                }
            });
            console.log(`‚úÖ –ü—Ä–∏–º–µ–Ω–µ–Ω –∫–æ–º–ø–ª–µ–∫—Ç –∏–∑ —Å–ª–æ—Ç–∞ ${slot}`);
        }
    }

    // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∏ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∞–∫—Ç–∏–≤–Ω–æ–≥–æ –∫–æ–º–ø–ª–µ–∫—Ç–∞ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã
    document.addEventListener("DOMContentLoaded", () => {
        const activeSet = localStorage.getItem("moswar_active_set");
        if (activeSet) {
            applySavedSet(activeSet); // –ù–æ–≤–∞—è —Å—Ç—Ä–æ–∫–∞: –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–æ—Å–ª–µ–¥–Ω–∏–π –∞–∫—Ç–∏–≤–Ω—ã–π –∫–æ–º–ø–ª–µ–∫—Ç
            updateSetIndicator(); // –ù–æ–≤–∞—è —Å—Ç—Ä–æ–∫–∞: –û–±–Ω–æ–≤–ª—è–µ–º –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä
        }

        // –î–æ–±–∞–≤–ª—è–µ–º MutationObserver –¥–ª—è –æ–±–µ—Å–ø–µ—á–µ–Ω–∏—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –∫–Ω–æ–ø–æ–∫ –∏ –ø–æ–≤—Ç–æ—Ä–Ω–æ–≥–æ –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è –∫–æ–º–ø–ª–µ–∫—Ç–æ–≤ –ø—Ä–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏
        const observer = new MutationObserver(() => {
            const target = document.querySelector("#content > div > div.block-bordered");
            if (typeof Ut === "function" && target && !target.querySelector("button")) {
                console.log("[üß™] –í—ã–∑–æ–≤ Ut() –¥–ª—è –≤—Å—Ç–∞–≤–∫–∏ –∫–Ω–æ–ø–æ–∫");
                Ut();
                if (activeSet) applySavedSet(activeSet); // –ù–æ–≤–∞—è —Å—Ç—Ä–æ–∫–∞: –ü–µ—Ä–µ–ø—Ä–∏–º–µ–Ω—è–µ–º –∫–æ–º–ø–ª–µ–∫—Ç –ø–æ—Å–ª–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è UI
                updateSetIndicator(); // –ù–æ–≤–∞—è —Å—Ç—Ä–æ–∫–∞: –û–±–Ω–æ–≤–ª—è–µ–º –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä –ø–æ—Å–ª–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è UI
            }
        });
        observer.observe(document.body, { childList: true, subtree: true });

        // –î–æ–±–∞–≤–ª—è–µ–º –∫–Ω–æ–ø–∫–∏ –∏–ª–∏ –¥—Ä—É–≥–∏–µ —ç–ª–µ–º–µ–Ω—Ç—ã UI, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
        if (typeof Ut === "function") {
            Ut();


	   }
    });


