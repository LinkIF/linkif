                try {
                    // –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç–æ–ø–ª–∏–≤–æ
                    const fuel = await fetch(`/automobile/car/${carId}/`, {
                        credentials: "include"
                    }).then(r => r.text());

                    const fuelMatch = fuel.match(/–¢–æ–ø–ª–∏–≤–æ:[^0-9]*([0-9]+)/i);
                    const currentFuel = fuelMatch ? parseInt(fuelMatch[1], 10) : 0;

                    // –ó–∞–ø—Ä–∞–≤–ª—è–µ–º –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
                    if (currentFuel === 0) {
